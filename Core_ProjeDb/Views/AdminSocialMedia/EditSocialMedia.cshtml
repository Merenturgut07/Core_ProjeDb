@model EntityLayer.Concrate.SocialMedia
@{
    ViewData["Title"] = "EditSocialMedia";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



@using (Html.BeginForm("EditSocialMedia", "AdminSocialMedia", FormMethod.Post))
{
	<div class="col-12 grid-margin stretch-card">
		<div class="card">
			<div class="card-body">
				<p class="card-description"> Hesap Güncelleme  Paneli </p>
				<form class="forms-sample">

					<div class="form-group">
						<label>Id</label>
						@Html.TextBoxFor(x => x.SocialMediaId, new { @class = "form-control" })

					</div>

					<div class="form-group">
						<label>Başlık</label>
						@Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
					</div>

					<div class="form-group">
						<label>Url</label>
						@Html.TextBoxFor(x => x.Url, new { @class = "form-control" })
					</div>

					<div class="form-group">
						<label>İkon</label>
						@Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
					</div>

					<button type="submit" class="btn btn-primary mr-2">Güncelle</button>
					<button class="btn btn-dark">İptal</button>
				</form>
			</div>
		</div>
	</div>

}






